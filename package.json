{
	"name": "iracing",
	"version": "1.0.0",
	"description": "Node.js lib for iRacing data api",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"api": "node --env-file=./.env dist/index.js",
		"check": "biome check --write",
		"login": "node --env-file=./.env dist/login.js",
		"typecheck": "tsc --noEmit"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"tough-cookie": "4.1.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "20.11.30",
		"@types/tough-cookie": "4.0.5",
		"typescript": "5.4.3"
	}
}
